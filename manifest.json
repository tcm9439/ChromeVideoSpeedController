{
  "name": "MyVideoSpeedController",
  "version": "0.1.0",
  "description": "Let get customized playback speed!",
  "permissions": ["tabs"],
  "host_permissions": ["https://*/*"],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["jquery-3.6.0.min.js","contentScript.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "My Video Speed Controller",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
 },
  "manifest_version": 3
}